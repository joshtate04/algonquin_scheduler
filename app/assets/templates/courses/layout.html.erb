<div class="container">
  <div class="row">
    <div class="col-sm-3 dashboard-navigation">
      <h4>Course Navigation</h4>
      <ul class="nav nav-pills nav-stacked">
        <li ng-repeat="course in courses"><a ui-sref="courses.show({id: course.id})">{{course.name}}</a></li>
      </ul>
    </div>
    <div class="col-sm-9">
      <div class="courses-view-container">
        <div class="courses-ui-view" ui-view></div>
      </div>
    </div>
  </div>
</div>